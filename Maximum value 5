#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a[10],i,n,max;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);      
    }
    max=a[0];
    {
        for(i=0;i<n;i++)
        {
            if(max<a[i])
            {
                max=a[i];
            }
        }
        printf("%d",max);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
